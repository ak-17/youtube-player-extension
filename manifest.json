{
    "manifest_version": 2,
    "name": "YouTubePlayer",
    "version": "0.0.1",
    "description": "helps to change youtube videos from extension",
    "icons": {
        "16":"images/16.png",
        "48":"images/48.png",
        "128":"images/128.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": [
            "*://www.youtube.com/*"
          ],
          "js": ["content.js"]
        }
      ],
    "permissions":[
        "tabs"
    ]

}