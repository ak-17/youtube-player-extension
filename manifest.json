{
    "manifest_version": 2,
    "name": "test Yoututeb",
    "version": "0.0.1",
    "description": "helps to change youtube videos from extension",
    "icons": {
        "16": "images/16.png",
        "48":"images/48.png",
        "128":"images/128.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "content_scripts": [
        {
          "matches": [
            "*://www.youtube.com/*",
            "https://github.com/ak-17/*"
          ],
          "js": ["jquery-3.4.0.min.js","content.js"],
          "css":["content.css"],
          "run_at":"document_start"
        }
      ],
    "permissions":[
        "tabs",
        "https://github.com/ak-17/*"
    ]
}