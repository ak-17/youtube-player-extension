{
    "manifest_version": 2,
    "name": "DoIt",
    "version": "0.0.1",
    "description": "helps to orgainise your to do",
    "icons": {
        "16":"images/16.png",
        "48":"images/48.png",
        "128":"images/128.png"
    },
    "page_action":{
        "default_icon":"images/16.png",
        "default_popup": "popup.html",
        "default_title":"title of extenions"
    },
    "background": {
        "scripts": ["eventPage.js"],
        "persistent":false
    },
    "content_scripts":[
        {
            "matches": ["https://www.youtube.com/*","https://developer.chrome.com/*"],
            "js":["jquery-3.4.0.min.js","content.js"],
            "all_frames": true
        }
    ],
    "permissions": [
        "tabs",
        "https://www.youtube.com/*",
        "https://developer.chrome.com/*",
        "contextMenus"
    ]

}